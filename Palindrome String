#include <stdio.h>
int main() {
    char s[50];
    int i=0, j=0, flag=1;
    printf("Enter string: ");
    scanf("%s", s);
     while(s[j]!='\0')
        j++;
    j--;
    while(i<j) {
        if(s[i]!=s[j]) {
            flag=0;
            break;
        }
        i++; j--;
    }
    if(flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");
    return 0;
}
